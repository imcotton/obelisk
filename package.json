{
  "name": "obelisk",
  "version": "1.0.0",
  "description": "GitHub Contribution Sheet",
  "repository": {},
  "scripts": {
    "run.ts": "tsc -p source --rootDir source",
    "run.js": "rollup -c build/rollup.config.js",
    "run.js.min": "google-closure-compiler-js --languageIn=ES5 --no-rewritePolyfills --assumeFunctionWrapper -- dist/script/bundle.js > dist/script/bundle.min.js",
    "run.css": "postcss -c build/postcss.config.js",
    "run.misc": "coffee build/misc.coffee",
    "run.data": "browserify -p [ tsify -t ES6 -m commonjs --noEmitHelpers --importHelpers --skipDefaultLibCheck ] --no-bundle-external --node -e build/gen-data.ts | node",
    "run.server": "ecstatic --port $npm_package_config_port",
    "dev": "concurrently -k 'npm run run.server' 'npm run run.ts -- -w' 'npm run run.js -- -w' 'npm run run.css -- -w'",
    "build": "del dist && npm run run.ts -- -t ES5 && npm run run.js && npm run run.js.min ; npm run run.css ; npm run run.misc",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "config": {
    "port": 4000,
    "github": {
      "login": "imcotton",
      "token": "replace with OAuth token to increase rate limiting"
    }
  },
  "author": "Cotton H.",
  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "@types/jquery": "^2.0.40",
    "@types/node-fetch": "^1.6.7",
    "autoprefixer": "^6.7.4",
    "bluebird": "^3.4.7",
    "browserify": "^14.1.0",
    "coffee-script": "^1.12.4",
    "concurrently": "^3.3.0",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "del-cli": "^0.2.1",
    "ecstatic": "^2.1.0",
    "google-closure-compiler-js": "^20170124.0.0",
    "jquery": "^3.1.1",
    "lodash.zip": "^4.2.0",
    "node-fetch": "^1.6.3",
    "postcss-cli": "^2.6.0",
    "postcss-nesting": "^2.3.1",
    "primer-css": "^4.7.0",
    "read-pkg": "^2.0.0",
    "replace-in-file": "^2.3.2",
    "rollup": "^0.41.4",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "rxjs": "^5.2.0",
    "tsify": "^3.0.1",
    "tslib": "^1.5.0",
    "typescript": "2.1.x"
  }
}
