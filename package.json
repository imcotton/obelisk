{
  "name": "obelisk",
  "version": "1.0.0",
  "description": "GitHub Contribution Sheet",
  "repository": {},
  "scripts": {
    "run.ts": "tsc -p source --rootDir source",
    "run.js": "rollup -c build/rollup.config.js",
    "run.css": "postcss -c build/postcss.config.js",
    "run.misc": "coffee build/misc.coffee",
    "run.server": "ecstatic --port 4000",
    "dev": "concurrently -k 'npm run run.server' 'npm run run.ts -- -w' 'npm run run.js -- -w' 'npm run run.css -- -w'",
    "build": "del dist && npm run run.ts -- -t ES5 && npm run run.js ; npm run run.css ; npm run run.misc",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "author": "Cotton H.",
  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "@types/jquery": "^2.0.40",
    "autoprefixer": "^6.7.2",
    "bluebird": "^3.4.7",
    "coffee-script": "^1.12.3",
    "concurrently": "^3.2.0",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "del-cli": "^0.2.1",
    "ecstatic": "^2.1.0",
    "jquery": "^3.1.1",
    "lodash.zip": "^4.2.0",
    "postcss-cli": "^2.6.0",
    "postcss-nesting": "^2.3.1",
    "primer-css": "^4.7.0",
    "replace-in-file": "^2.2.2",
    "rollup": "^0.41.4",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-inject": "^2.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "rxjs": "^5.1.0",
    "tslib": "^1.5.0",
    "typescript": "2.1.x"
  }
}
